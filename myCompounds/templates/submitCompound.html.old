<script type="text/javascript">
var droppable_hint = null;
var dropped = new Array();
$(document).ready(function() {
	droppable_hint = $("#input-droppable").html();
	$(".user-object").draggable({
		helper: 'clone',
		opacity: 0.5	
	});
	$("#input-droppable").droppable({
		accept: ".user-object",
		activeClass: 'droppable-active',
		hoverClass: 'droppable-hover',
		drop: function(ev, ui) {
			if ($(this).data('clean')) {
				$(this).empty();
				$(this).data('clean', false);
			}
			$(this).append($(ui.draggable).clone());
			dropped.push($(ui.draggable).children(".user-object-name").text());
		}
	}).data('clean', true);
	$("input[name=clear]").click(function() {
		$("#input-droppable").html(droppable_hint);
		dropped = new Array();
		$("#input-droppable").data('clean', true);
	});
	$("input[name=submit_obj]").click(function() {
		$("input[name=object_name]").get(0).value = dropped.join("|");
	});
	$("#input-type-menu").tabs();
	$("input[type=submit]").click(function() {
		mpmetrics.track("start application", {'appname': '' });
		return true;
	});
});
</script>


<div id="input-type-menu"> 
    <ul style="display:inline-block">
    <li>
    <a href="#textarea">
    <span>Add by Copy-and-Paste</span></a></li>
    <li>
    <a href="#upload">
    <span>Add by Uploading a File</span></a></li>
    <li>
    <a href="#viewCompounds">
    <span>View My Compounds</span></a></li>
    </ul>
    
    
    <div id="textarea">
        <table width="100%">
        <tr>
        <td width="80%">
        <form action="process" method="post" enctype="multipart/form-data">
        <label for="str_input">Enter smiles entry:</label><br />
        <textarea rows="10" cols="80" id="str_input" name="str_input"></textarea><br />
        <input type="submit" value="Submit" />
        </form>
        </td>
        <td width="20%" style="border-left:1px solid #aaf">
        <a href="#" class="abutton" id="use_sample">Use 129 Sample Compounds</a>
        </td>
        </tr>
        </table>
        
        <div class="box">
            <div class="title">Expected Format</div>
            <pre>smiles1 name1
            smiles2 name2</pre>
        </div>
        <div class="box">
            <div class="title">Example</div>
            <pre>C(Nc1cc(OC)ccc1OC)(c1cc(I)ccc1)=O ChemBridge_DiverSet_20k
            N1(C(SC(\C1=O)=C\c1cc(OC)c(cc1)OCCCCC)=S)CC(=O)OC Sample_2</pre>
        </div>
    </div>
    
    <div id="upload">
        <form action="process" method="post" enctype="multipart/form-data">
        <label for="smi_file">SMILES or SDF file:</label>
        <input size="40" type="file" id="smi_file" name="smi_file" />
        <p>
        <input type="submit" value="Upload File" />
        </p>
        </form>
        <div class="box">
            <div class="title">Sample SMILES File</div>
            <p>Download a <a href="/static/example_db.smi">sample SMILES file</a> containing 129 unnamed compounds.</p>
        </div>
        <div class="box">
            <div class="title">Expected Format</div>
            <pre>smiles1 name1
            smiles2 name2</pre>
        </div>
    </div>
    
    <div id="viewCompounds">
        User uploaded compound viewer placeholder.
    </div>
</div>

